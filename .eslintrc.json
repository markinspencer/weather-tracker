{
  "env": {
    "browser": true,
    "node": false,
    "jasmine": false
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "linebreak-style": ["error", "windows"],
    "no-use-before-define": ["error", { "functions": false, "classes": true }]
  }
}
